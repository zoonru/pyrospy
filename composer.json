{
    "name": "zoonru/pyrospy",
    "description": "Adapter from phpspy to pyroscope",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "phpspy",
        "pyroscope",
        "profiler",
        "adapter"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "symfony/console": "^5|^6",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "autoload": {
        "psr-4": {
            "Zoon\\PyroSpy\\": "app/"
        }
    }
}
